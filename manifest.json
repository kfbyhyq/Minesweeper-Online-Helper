{  
    "manifest_version": 3,  
    "name": "Minesweeper Online Helper",  
    "version": "3.8.2",  
    "description": "A simple extension for Minesweeper Online.",  
    "permissions": ["activeTab", "scripting", "storage", "tabs", "alarms"],
    "action": {
        "default_popup": "displayPage.html",  
        "default_icon": {  
            "16": "moh16.png",  
            "48": "moh48.png",  
            "128": "moh128.png"  
        }  
    },
    "icons": {
        "16": "moh16.png",  
        "48": "moh48.png",  
        "128": "moh128.png"  
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
}